<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="skinAble">
        <!--是否启用换肤,默认为true；可通过{SkinManager.setSkinGlobalEnable}设置全局开关-->
        <attr name="skinEnable" format="boolean" />
        <!--用于标识拓展SkinMethodHolder时是否启用after或proxy操作-->
        <attr name="skinMethodEnable" format="boolean" />
        <!--用于标识是否启用Dimen替换操作-->
        <attr name="skinDimenEnable" format="boolean" />
        <attr name="skinTag" format="string" />
    </declare-styleable>
</resources>