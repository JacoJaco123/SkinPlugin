<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="skinAble">
        <!--是否启用换肤,默认为true；可通过{SkinManager.setSkinGlobalEnable}设置全局开关-->
        <attr name="skinEnable" format="boolean" />
        <!--用于标识拓展SkinMethodHolder时是否启用的after或proxy操作-->
        <attr name="skinMethodTag" format="string" />
        <!--todo-->
        <attr name="skinTag" format="string" />
    </declare-styleable>

    <!--View的tag key-->
    <item name="skinMethodTagID" type="id" />
</resources>